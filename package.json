    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-parser.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-parser/issues"
  },
